{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right"><a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </div>
  
  <div class="container-fluid">
    <div class="row">
     <div class="col-md-12">
      <div class="panel panel-default">
       <div class="panel-heading">
        <h3 class="panel-title"><i class="fa fa-cart-arrow-down"></i> {{ text_order }}</h3>
       </div>
      </div>
     </div>
    </div>
    
    <div class="row">
      <div class="col-md-12">
      <div class="panel panel-default">
         <div class="panel-heading">
            <h3 class="panel-title"><i class="fa fa-external-link-square"></i> {{ text_track_header }}</h3>
          </div>
          <div class="panel-body">
            <!--DHL Tracking Links !-->
            <div class="table-responsive">
                <table class="table table-bordered table-hover">
                  <thead>
                    <tr>        
                    <td class="text-left">{{ column_awb_number }}</td>
                    <td class="text-left">{{ column_customer_link }}</td>
                    <td class="text-right">{{ column_tracking_action }}</td>
                  </tr>
                </thead>
                <tbody>
                {% if alllawbs %}
                {% for awb in alllawbs %}
                  <tr>
                     <td class="text-left">{{ awb.airway_bill_number }}</td>
                     <td class="text-left"><strong>http://www.dhl.com/en/express/tracking.shtml?AWB={{ awb.airway_bill_number }}&brand=DHL</strong></td>
                     <td class="text-right">
                     <a href="http://www.dhl.com/en/express/tracking.shtml?AWB={{ awb.airway_bill_number }}&brand=DHL" target="_blank" data-toggle="tooltip" title="{{ button_track }}" class="btn btn-info">{{ button_track }} <i class="fa fa-external-link-square"></i></a> 
                     </td>
                  </tr>
                  {% endfor %}
                  {% else %}
                  <tr>
                    <td class="center" colspan="5">{{ text_no_results }}</td>
                  </tr>
                  {% endif %}
                  </tbody>
                </table>
              </div>
          </div>
       </div>
     </div>
    </div><!--end row!-->
  </div><!--end container fluid!-->
</div> <!--end content!-->
{{ footer }} 