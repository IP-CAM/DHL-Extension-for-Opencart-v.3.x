{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <button type="submit" form="form-shipping" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
        <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </div>
  <div class="container-fluid">
    {% if error_warning %}
    <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
      <button type="button" class="close" data-dismiss="alert">&times;</button>
    </div>
    {% endif %}
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><i class="fa fa-pencil"></i> {{ text_edit }}</h3>
      </div>

      <div class="panel-body">
        <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-saffwebdhl" class="form-horizontal">
      
     <ul class="nav nav-tabs">
      <li class="active"><a href="#tab-dhl-settings" data-toggle="tab">{{ text_dhl_settings }}</a></li>
      <li><a href="#tab-dhl-products" data-toggle="tab">{{ text_dhl_products }}</a></li>
      <li><a href="#tab-shipper-company" data-toggle="tab">{{ text_shipper_details }}</a></li>
      <li><a href="#tab-shipping-origin" data-toggle="tab">{{ text_shipping_origin }}</a></li>
      <li><a href="#tab-contact-person" data-toggle="tab">{{ text_contact_person }}</a></li>
      <li><a href="#tab-dhl-logs" data-toggle="tab">{{ text_dhl_logs }}</a></li>
      <li><a href="#tab-general-settings" data-toggle="tab">{{ text_general_settings }}</a></li>
	</ul>
    
    <div class="tab-content">
      <div class="tab-pane active" id="tab-dhl-settings">
         <div class="form-group">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_test_mode"><span data-toggle="tooltip" title="{{ help_test_mode }}">{{ entry_test_mode }}</span></label>
            <div class="col-sm-10">              
              <label class="radio-inline">
                {% if shipping_saffwebdhl_test_mode == 1 %}
                <input type="radio" name="shipping_saffwebdhl_test_mode" value="1" checked="checked" />
                {{ text_testing }}
                {% else %}
                <input type="radio" name="shipping_saffwebdhl_test_mode" value="1" />
                {{ text_testing }}
                {% endif %}
              </label>
              <label class="radio-inline">
                {% if shipping_saffwebdhl_test_mode == 0 %}
                <input type="radio" name="shipping_saffwebdhl_test_mode" value="0" checked="checked" />
                {{ text_production }}
                {% else %}
                <input type="radio" name="shipping_saffwebdhl_test_mode" value="0" />
                {{ text_production }}
                {% endif %}
              </label>
            </div>
          </div>
      
          <div class="form-group required">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_access_id"><span data-toggle="tooltip" title="{{ help_access_id }}">{{ entry_access_id }}</span></label>
            <div class="col-sm-10">
              <input type="text" name="shipping_saffwebdhl_access_id" value="{{ shipping_saffwebdhl_access_id }}" id="shipping_saffwebdhl_access_id" class="form-control" />
              {% if error_access_id %}
              <div class="text-danger">{{ error_access_id }}</div>
              {% endif %}
            </div>
          </div>      
      
          <div class="form-group required">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_password"><span data-toggle="tooltip" title="{{ help_password }}">{{ entry_password }}</span></label>
            <div class="col-sm-10">
              <input type="password" name="shipping_saffwebdhl_password" value="{{ shipping_saffwebdhl_password }}" id="shipping_saffwebdhl_password" class="form-control" />
              {% if error_password %}
              <div class="text-danger">{{ error_password }}</div>
              {% endif %}
            </div>
          </div>      
      
          <div class="form-group required">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_account_number"><span data-toggle="tooltip" title="{{ help_account_number }}">{{ entry_account_number }}</span></label>
            <div class="col-sm-10">
              <input type="text" name="shipping_saffwebdhl_account_number" value="{{ shipping_saffwebdhl_account_number }}" id="shipping_saffwebdhl_account_number" class="form-control" />
             {% if error_account_number %}
              <div class="text-danger">{{ error_account_number }}</div>
             {% endif %}
            </div>
          </div>  
          
        <div class="form-group">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_dhl_region">{{ entry_dhl_region }}</label>
            <div class="col-sm-10">
              <select name="shipping_saffwebdhl_dhl_region" class="form-control">
                {% for option_eight in dhlregion %}
                {% if option_eight.option_value == shipping_saffwebdhl_dhl_region %}
