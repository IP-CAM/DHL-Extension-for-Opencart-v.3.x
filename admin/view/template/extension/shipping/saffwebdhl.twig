                    {% else %}
                    <input type="checkbox" name="shipping_saffwebdhl_dhl_domestic[]" value="{{ domestic.id }}" />
                    [{{ domestic.global_product_code }}] {{ domestic.dhl_product_name }}
                    {% endif %}
                  </label>
                </div>
                {% endfor %}
              </div>
              {% if error_dhl_domestic %}
              <div class="text-danger">{{ error_dhl_domestic }}</div>
              {% endif %}
            </div>
          </div> 
          
