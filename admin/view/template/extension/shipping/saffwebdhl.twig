              {% endif %}
            </div>
          </div>         
          
          <div class="form-group required">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_company_name">{{ entry_company_name }}</label>
            <div class="col-sm-10">
              <input type="text" name="shipping_saffwebdhl_company_name" value="{{ shipping_saffwebdhl_company_name }}" id="shipping_saffwebdhl_company_name" class="form-control" />
              {% if error_company_name %}
