                {% for international in dhlproducts %}
                <div class="checkbox">
                  <label>
                    {% if international.id in dhl_international %}
                    <input type="checkbox" name="shipping_saffwebdhl_dhl_international[]" value="{{ international.id }}" checked="checked" />
                    [{{ international.global_product_code }}] {{ international.dhl_product_name }}
                    {% else %}
                    <input type="checkbox" name="shipping_saffwebdhl_dhl_international[]" value="{{ international.id }}" />
                    [{{ international.global_product_code }}] {{ international.dhl_product_name }}
                    {% endif %}
                  </label>
                </div>
                {% endfor %}
              </div>
              {% if error_dhl_international %}
              <div class="text-danger">{{ error_dhl_international }}</div>
              {% endif %}
            </div>
          </div> 
          
          <div class="form-group">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_label_image_format">{{ entry_label_image_format }}</label>
            <div class="col-sm-10">
              <select name="shipping_saffwebdhl_label_image_format" id="input-status" class="form-control">
                {% for option_four in imageformat %}
                {% if option_four.option_value == shipping_saffwebdhl_label_image_format %}
                <option value="{{ option_four.option_value }}" selected="selected">{{ option_four.option_name }}</option>
                {% else %}
                <option value="{{ option_four.option_value }}">{{ option_four.option_name }}</option>
                {% endif %}
                {% endfor %}              
              </select>
            </div>
          </div>
          
          <div class="form-group">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_label_image_size">{{ entry_label_image_size }}</label>
            <div class="col-sm-10">
              <select name="shipping_saffwebdhl_label_image_size" id="input-status" class="form-control">
                {% for option_five in imagesize %}
                {% if option_five.option_value == shipping_saffwebdhl_label_image_size %}
                <option value="{{ option_five.option_value }}" selected="selected">{{ option_five.option_name }}</option>
                {% else %}
                <option value="{{ option_five.option_value }}">{{ option_five.option_name }}</option>
                {% endif %}
                {% endfor %}              
              </select>
            </div>
          </div> 
 
          <div class="form-group">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_package_type">{{ entry_package_type }}</label>
            <div class="col-sm-10">
              <select name="shipping_saffwebdhl_package_type" id="input-status" class="form-control">
                {% for option_six in packagetype %}
                {% if option_six.option_value == shipping_saffwebdhl_package_type %}
                <option value="{{ option_six.option_value }}" selected="selected">{{ option_six.option_name }}</option>
                {% else %}
                <option value="{{ option_six.option_value }}">{{ option_six.option_name }}</option>
                {% endif %}
                {% endfor %}              
              </select>
            </div>
          </div>

    </div>
    <div class="tab-pane" id="tab-shipper-company">
        <div class="form-group">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_shipper_identification_type">{{ entry_shipper_identification_type }}</label>
            <div class="col-sm-10">
              <select name="shipping_saffwebdhl_shipper_identification_type" class="form-control">
                <option value="">-- None --</option>
                {% for option_two in identification %}
                {% if option_two.option_value == shipping_saffwebdhl_shipper_identification_type %}
                <option value="{{ option_two.option_value }}" selected="selected">{{ option_two.option_name }}</option>
                {% else %}
                <option value="{{ option_two.option_value }}">{{ option_two.option_name }}</option>
                {% endif %}
                {% endfor %}              
            </select>
            </div>
          </div>
          
         <div class="form-group">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_shipper_ein_ssn_number">{{ entry_shipper_ein_ssn_number }}</label>
            <div class="col-sm-10">
              <input name="shipping_saffwebdhl_shipper_ein_ssn_number" value="20-0049202" placeholder="{{ entry_shipper_ein_ssn_number }}" id="input-name" class="form-control" type="text">
            </div>
          </div>
          
          <div class="form-group">
            <label class="col-sm-2 control-label" for="shipping_saffwebdhl_shipper_payment_type">{{ entry_shipper_payment_type }}</label>
            <div class="col-sm-10">
              <select name="shipping_saffwebdhl_shipper_payment_type" id="input-status" class="form-control">
                {% for option_three in paymenttype %}
                {% if option_three.option_value == shipping_saffwebdhl_shipper_payment_type %}
                <option value="{{ option_three.option_value }}" selected="selected">{{ option_three.option_name }}</option>
                {% else %}
                <option value="{{ option_three.option_value }}">{{ option_three.option_name }}</option>
                {% endif %}
                {% endfor %}              
              </select>
            </div>
          </div>
          
          <div class="form-group">
            <label class="col-sm-2 control-label" for="export_license">{{ entry_export_license }}</label>
            <div class="col-sm-10">
               <input type="text" name="shipping_saffwebdhl_export_license" value="20-0049202" placeholder="{{ entry_export_license }}" id="input-export-license" class="form-control" />
          </div>
         </div>

          <div class="form-group">
            <label class="col-sm-2 control-label" for="import_license">{{ entry_import_license }}</label>
            <div class="col-sm-10">
               <input type="text" name="shipping_saffwebdhl_import_license" value="20-0049202" placeholder="{{ entry_import_license }}" id="input-import-license" class="form-control" />
          </div>
         </div>

          <div class="form-group">
            <label class="col-sm-2 control-label" for="schedule_b"><span data-toggle="tooltip" title="{{ help_schedule_b }}">{{ entry_schedule_b }}</span></label>
            <div class="col-sm-10">
               <input type="text" name="shipping_saffwebdhl_schedule_b" value="123456" placeholder="{{ entry_schedule_b }}" id="input-schedule-b" class="form-control" />
          </div>
         </div>

    </div>
    <div class="tab-pane" id="tab-shipping-origin">
          
