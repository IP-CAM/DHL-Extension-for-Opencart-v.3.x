            <div class="col-sm-10">
              <select name="shipping_saffwebdhl_duty_payment_type" class="form-control">
                {% for option_one in dutypayment %}
                {% if option_one.option_value == shipping_saffwebdhl_duty_payment_type %}
                <option value="{{ option_one.option_value }}" selected="selected">{{ option_one.option_name }}</option>
                {% else %}
                <option value="{{ option_one.option_value }}">{{ option_one.option_name }}</option>
                {% endif %}
                {% endfor %}              
            </select>
            </div>
          </div>        
        
    </div>
    <div class="tab-pane" id="tab-dhl-products">
